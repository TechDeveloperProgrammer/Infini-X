
# .env.example - Example environment variables for Infini-X

# Copy this file to .env and fill in your actual values.
# This file should NOT be committed to version control if it contains sensitive information.

# Database Credentials
DB_USER=infini_x_user
DB_PASSWORD=your_db_password

# API Keys (example)
OPENAI_API_KEY=your_openai_api_key
GOOGLE_API_KEY=your_google_api_key

# Application Settings
APP_ENV=development # production, staging, development
APP_DEBUG=true # true or false

# Paths
MODEL_CACHE_DIR=/opt/infini-x/models
